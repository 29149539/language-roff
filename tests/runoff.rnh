.! SUMMARY OF RUNOFF COMMANDS
.fi .fill
.nf .nofill
.ad .adjust
.nj .nojust
.df .define format  [name] [pos] [field-defs …]
	[pos]           LEFT | RIGHT | CENTER
	[field-def]     [type]([letter]…[letter])
		<type>      L | R | C | F | J
.ef .end format
.fm .format                  <name>
.hy .hyphenate
.nh .nohyphen
.gl .glossary                W … W
.hb .hyphenation break       N
.la .layout                  N*, N*, N*, N*, N*, N*
.pl .page layout             N*, N*, N*
.ll .line layout             N*, N*, N*, N*
.rm .reduce margin           N*, N*
.em .expand margin           N*, N*
.er .end reduction
.ps .paragraph spacing       N
.pi .paragraph indentation   N
.pu .paragraph undentation   N
.ce .center
.in .indent N*
.un .undent N*
.ma .margin
.he .header S
.hm .heading mode P
.pm .paging mode P
.op .odd page
.pa .page N*
.ej .eject N*
.ss .single space
.ds .double space
.sp .space N*
.fs .figure spacing N
.br .break
.bg .begin group
.eg .end group
.ul .underline
.li .literal
.es .escape C
.sh .shift C*
.tc .tab character C*
.dc .define command W
.ec .end command
.cl .call W
.ix .index S, S*
