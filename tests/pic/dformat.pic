.begin dformat
style bitwid 0.12
Short
	0 S
	1-15 Integer
	
Int
	0 S
	1-31 Integer

Float
	0 S
	1-7 Exponent
	8-31 Fraction
.end


.begin dformat
style bitwid 0.8
RR
	0-7 Opcode
	8-11 R1
	12-15 R2
RX
	0-7 Opcode
	8-11 R1
	12-15 X2
	16-19 B2
	20-31 D2
.end


.begin dformat
style fill on
style bitwid 0.05
style recspread 0
style addr off
style recht 0.2
style addr reset
noname
	0-63 Double Word
noname
	0-31 Word
	32-63 Word
noname
	0-15 Half Word
	16-31 Half Word
	32-47 Half Word
	48-63 Half Word
.end


.begin dformat
style bitwid .2
Record 1
	F1: 1-8 Field 1
Record 2
	F2: 1-16 Field 2
pic line dotted from F1.sw to F2.nw
pic line dotted from F1.se to F2.ne
.end

.ps +10
.begin dformat
style bitwid 0.15
style recht 0.6
style recspread 0.3
style addrdelta	8
style addrht 0.1
style addr left
RR
	0-7 Opcode
	8-11 R1
	12-15 R2
RX
	0-7 Opcode
	8-11 R1
	12-15 X2
	16-19 B2
	20-31 D2
.end
.ps -10


.begin dformat
style bitwid 0.08
style charwid 0
style recspread 0.3
noname
		--16 Frame
		--16 Frame
	A1:	--16 Frame
		--16 Frame
		--8-dash ...
noname
	A2:	8--8 Flags
		8--8 Status
		--8 @roman Chunk sub 1@
	B1:	--8 @roman Chunk sub 2@
		--8-dash ...
		--8 @roman Chunk sub m@
		16--16 CRC
	A3:	8--8 Flags
noname
	B2:	8--8 @roman Data sub 1@
		8--8 @roman Data sub 2@
		8--8 @roman Data sub 3@
		8--8 @roman Data sub 4@
		--8-dash ...
		8--8 @roman Data sub {n-1}@
		8--8 @roman Data sub n@
		6--6 Length
	B3:	10--10 Channel #
pic line dotted from A1.sw to A2.nw
pic line dotted from A1.se to A3.ne
pic line dotted from B1.sw to B3.nw
pic line dotted from B1.se to B3.ne
.end
